package chapter5;
import java.util.Scanner;
public class m341fix {
	public static void main(String [ ] args)

    
	{
Scanner s = new Scanner(System.in);
System.out.print("Extensions =   ");
int num = s.nextInt();
System.out.print("Description of extensions is...  ");
String word = s.next();
extensions(num,word);	
}	

public static void extensions(int num,String word)
{
double count=1;
while(count<=num) {
	count++;
System.out.print("\n"+ word);}
}}



