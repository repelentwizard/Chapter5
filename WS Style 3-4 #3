package chapter5;
import java.util.Scanner;
public class ws343 {
	public static void main(String [ ] args)

    
	{
int num;
Scanner s = new Scanner(System.in);
System.out.print("Enter a number:  ");
num = s.nextInt();
extension(num);
	}

public static void extension(int num)
{
int total=num*2;
System.out.print(total);
}}
